{
  "name": "timeseriesadmin",
  "description": "Administration and querying interface for InfluxDB databases",
  "author": "Jan Grzegorowski <grzegorowski@gmail.com> (https://grzegorowski.com)",
  "version": "0.1.6",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "4.7.2",
    "@material-ui/icons": "4.5.1",
    "@types/classnames": "2.2.9",
    "@types/jest": "24.0.23",
    "@types/lodash": "4.14.149",
    "@types/mui-datatables": "2.13.1",
    "@types/node": "12.12.14",
    "@types/papaparse": "5.0.2",
    "@types/react": "16.9.15",
    "@types/react-dom": "16.9.4",
    "@types/react-inspector": "3.0.0",
    "@types/webpack-env": "1.14.1",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client": "2.6.4",
    "apollo-link": "1.2.13",
    "axios": "0.19.0",
    "compare-versions": "3.5.1",
    "date-fns": "2.8.1",
    "final-form": "4.18.6",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "history": "4.10.1",
    "influx-api": "0.1.4",
    "lodash": "4.17.15",
    "mui-datatables": "2.13.3",
    "papaparse": "5.1.0",
    "qs": "6.9.1",
    "react": "16.12.0",
    "react-apollo": "3.1.3",
    "react-dom": "16.12.0",
    "react-final-form": "6.3.3",
    "react-inspector": "4.0.0",
    "react-popper": "1.3.6",
    "react-scripts": "3.3.0",
    "react-use": "13.11.0",
    "typescript": "3.7.3"
  },
  "scripts": {
    "clean": "yarn rimraf dist",
    "start": "./update-env.sh && react-scripts start",
    "build": "./update-env.sh && react-scripts build",
    "db:start": "docker-compose -f dev-db/docker-compose.yml up",
    "test": "TZ=Europe/Warsaw react-scripts test src --bail --runInBand",
    "lint": "eslint './src/**/*.{ts,tsx,js,jsx}'",
    "electron:dev": "ELECTRON_IS_DEV=1 ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron": "ELECTRON_IS_DEV=1 electron .",
    "electron:stag": "electron-builder -m -c.compression=store -c.extraMetadata.main=build/electron.js",
    "electron:prod": "yarn dist:mac",
    "dist:docker": "yarn build && sh -ac 'source ./.env; docker build . -t timeseriesadmin/timeseriesadmin:$REACT_APP_VERSION'",
    "release:docker": "sh -ac 'source ./.env; docker tag timeseriesadmin/timeseriesadmin:$REACT_APP_VERSION timeseriesadmin/timeseriesadmin:latest && docker push timeseriesadmin/timeseriesadmin:latest && docker push timeseriesadmin/timeseriesadmin:$REACT_APP_VERSION'",
    "dist:mac": "yarn build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "dist:win": "yarn build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "dist:linux": "yarn build && electron-builder -l -c.extraMetadata.main=build/electron.js",
    "dist:all": "yarn dist:docker && electron-builder -mwl -c.extraMetadata.main=build/electron.js",
    "cypress:run": "cypress run --browser chrome",
    "cypress:open": "cypress open",
    "ci:test": "TZ=Europe/Warsaw CI=true react-scripts test --coverage --runInBand --bail  && codecov -t 1034d70c-6870-4421-986f-6ac405850292",
    "ci:cypress": "yarn cypress:run --browser chrome"
  },
  "devDependencies": {
    "@apollo/react-testing": "3.1.3",
    "@testing-library/cypress": "5.0.2",
    "@testing-library/react": "9.3.2",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "babel-eslint": "10.0.3",
    "babel-polyfill": "6.26.0",
    "codecov": "3.6.1",
    "cypress": "3.7.0",
    "electron": "7.1.3",
    "electron-builder": "21.2.0",
    "electron-debug": "3.0.1",
    "eslint": "6.7.2",
    "eslint-plugin-react": "7.17.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "wait-on": "3.3.0"
  },
  "main": "public/electron.js",
  "_homepage": "homepage value should not be changed or Electron will not be able to localize source files",
  "homepage": ".",
  "build": {
    "appId": "com.grzegorowski.timeseriesadmin",
    "productName": "Time Series Admin",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": "dmg",
      "category": "utilities",
      "type": "development"
    },
    "win": {
      "target": "nsis",
      "icon": "./assets/icon.png"
    },
    "linux": {
      "target": "deb",
      "category": "Development"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/registerServiceWorker.ts"
    ]
  }
}
